{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  },
  "cSpell.words": [
    "nativewind"
  ],
  "tailwindCSS.experimental.classRegex": [
    "className=\"([^\"]*)\"",
    "className={`([^`]*)`}",
    "class=\"([^\"]*)\"",
    "class={`([^`]*)`}"
  ]
}