{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  },
  "cSpell.words": [
    "nativewind"
  ],
  "tailwindCSS.experimental.classRegex": [
    "className=\"([^\"]*)\"",
    "className={`([^`]*)`}",
    "class=\"([^\"]*)\"",
    "class={`([^`]*)`}"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true
  }
}